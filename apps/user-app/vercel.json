{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "build": {
    "env": {
      "PRISMA_GENERATE_DATAPROXY": "false"
    },
    "commands": {
      "preinstall": "cd ../.. && pnpm install --filter user-app...",
      "build": "cd ../.. && pnpm build --filter=user-app"
    }
  }
}
